<Window x:Class="InformacioniSistemBolnice.TerminiPacijentaProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
        Title="TerminiPacijentaProzor" Height="480" Width="500">
    <Canvas>
        <DataGrid x:Name="listaZakazanihTermina" Height="405" Width="391" Canvas.Top="59" CanUserAddRows="False" Canvas.Left="101"/>
        <Button x:Name="zakaziDugme" Canvas.Left="129" Canvas.Top="19" Width="64" Click="zakaziDugme_Click">Zakazi</Button>
        <Button x:Name="otkaziDugme" Canvas.Left="225" Canvas.Top="19" Width="64" Content="Otkazi" Click="otkaziDugme_Click"/>
        <Button x:Name="pomeriDugme" Canvas.Left="322" Canvas.Top="19" Width="64" Content="Pomeri" Click="pomeriDugme_Click"/>
        <Button x:Name="infoDugme" Canvas.Left="423" Canvas.Top="19" Width="64" Content="Info" Click="infoDugme_Click"/>
        <Button Content="Vesti" Canvas.Left="20" Canvas.Top="69" Width="60" Click="prikaziVesti_Click"/>
    </Canvas>
</Window>
