            <TextBox x:Name="JMBGUnos" Canvas.Left="183" Canvas.Top="137" Text="" TextWrapping="Wrap" Width="120"/>
            <Label x:Name="prezimeLabela" Canvas.Left="183" Canvas.Top="170"  Width="120" Height="22"/>
            <TextBox x:Name="imeRoditeljaUnos" Canvas.Left="183" Canvas.Top="205" Text="" TextWrapping="Wrap" Width="120"/>
            <Label x:Name="imeLabela" Canvas.Left="183" Canvas.Top="232"   Width="120" Height="20"/>
            <Label x:Name="adresaLabela" Canvas.Left="183" Canvas.Top="304"   Width="120" Height="20"/>
