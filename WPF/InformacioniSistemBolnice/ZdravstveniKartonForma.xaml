            <TextBox x:Name="datumRodjenjaUnos_Copy" Canvas.Left="573" Canvas.Top="341" Text="" TextWrapping="Wrap" Width="120"/>
            <TextBox x:Name="ulicaIBrojUnos_Copy" Canvas.Left="573" Canvas.Top="407" Text="" TextWrapping="Wrap" Width="120"/>
            <TextBox x:Name="PrezimeUnos_Copy1" Canvas.Left="573" Canvas.Top="209" Text="" TextWrapping="Wrap" Width="120"/>
            <Button x:Name="potvrdiDugme" Content="Kreiraj zdravstveni karton" Grid.Column="1" Width="149" Canvas.Left="547" Canvas.Top="473" Click="kreirajZdravstveniKarton_Click"/>
        </Canvas>


    </Grid>
</Window>