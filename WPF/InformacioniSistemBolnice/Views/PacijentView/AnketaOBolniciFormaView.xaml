            <RadioButton GroupName="Azurnost" x:Name="Azurnost5" Content="" Canvas.Left="140" Canvas.Top="242" />
            <RadioButton GroupName="Azurnost" x:Name="Azurnost4" Content="" Canvas.Left="220" Canvas.Top="242" />
            <RadioButton GroupName="Azurnost" x:Name="Azurnost3" IsChecked="True" Content="" Canvas.Left="300" Canvas.Top="242" />
            <RadioButton GroupName="Azurnost" x:Name="Azurnost2" Content="" Canvas.Left="380" Canvas.Top="242" />
            <RadioButton GroupName="Azurnost" x:Name="Azurnost1" Content="" Canvas.Left="460" Canvas.Top="242" />
