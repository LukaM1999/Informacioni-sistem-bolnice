            <RadioButton GroupName="Ljubaznost" x:Name="Ljubaznost4" Content="" Canvas.Left="220" Canvas.Top="77" />
            <Label Content="Zadovoljan" Canvas.Left="192" Canvas.Top="28" Height="44" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="75"/>
            <RadioButton GroupName="Ljubaznost" x:Name="Ljubaznost3" IsChecked="True" Content="" Canvas.Left="300" Canvas.Top="77" />
            <Label Content="Uzdrzan" Canvas.Left="272" Canvas.Top="28" Height="44" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="75"/>
            <RadioButton GroupName="Ljubaznost" x:Name="Ljubaznost2" Content="" Canvas.Left="380" Canvas.Top="77" />
            <Label Content="Nezadovoljan" Canvas.Left="344" Canvas.Top="28" Height="44" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="91"/>
            <RadioButton GroupName="Ljubaznost" x:Name="Ljubaznost1" Content="" Canvas.Left="460" Canvas.Top="77" />
            <Label Content="Veoma&#xA;nezadovoljan" Canvas.Left="428" Canvas.Top="28" Height="44" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="84"/>
            <Label Content="Ljubaznost:" Canvas.Left="19" Canvas.Top="67"/>
        </Canvas>
        <Canvas x:Name="Profesionalizam">
            <RadioButton GroupName="Profesionalizam" x:Name="Profesionalizam5" Content="" Canvas.Left="140" Canvas.Top="117" />
            <RadioButton GroupName="Profesionalizam" x:Name="Profesionalizam4" Content="" Canvas.Left="220" Canvas.Top="117" />
            <RadioButton GroupName="Profesionalizam" x:Name="Profesionalizam3" IsChecked="True" Content="" Canvas.Left="300" Canvas.Top="117" />
            <RadioButton GroupName="Profesionalizam" x:Name="Profesionalizam2" Content="" Canvas.Left="380" Canvas.Top="117" />
            <RadioButton GroupName="Profesionalizam" x:Name="Profesionalizam1" Content="" Canvas.Left="460" Canvas.Top="117" />
