            <RadioButton GroupName="Korisnost" x:Name="Korisnost4" Content="" Canvas.Left="220" Canvas.Top="283" />
            <RadioButton GroupName="Korisnost" x:Name="Korisnost3" IsChecked="True" Content="" Canvas.Left="300" Canvas.Top="283" />
