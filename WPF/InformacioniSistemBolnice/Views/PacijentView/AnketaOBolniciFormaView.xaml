        <Label Content="Korisnost:" Canvas.Left="19" Canvas.Top="273"/>
        <Label Content="Komentari:" Canvas.Left="19" Canvas.Top="328"/>
        <TextBox x:Name="Komentari" Style="{StaticResource PlaceHolder}" Tag="Unesite komentar o iskustvu pri poseti bolnice..." Canvas.Left="112" Canvas.Top="333" TextWrapping="Wrap" Height="131" Width="321"/>
        <Button x:Name="Potvrdi" Content="Pošalji" ToolTip="Potvrdite slanje ankete o bolnici" Canvas.Left="174" Canvas.Top="486" Width="70" Click="Potvrda"/>
        <Button x:Name="Odustani" IsCancel="True" ToolTip="Odustanite od slanja ankete o bolnici" Content="Odustani" Canvas.Left="298" Canvas.Top="486" Width="70" Click="Potvrda"/>
    </Canvas>
</Window>
