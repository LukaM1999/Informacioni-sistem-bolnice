        <Button x:Name="dugmePotvrdi" Content="Porvrdi" HorizontalAlignment="Left" Margin="328,188,0,0" VerticalAlignment="Top" Height="30" Width="72" Click="dugmePotvrdi_Click"/>

    </Grid>
