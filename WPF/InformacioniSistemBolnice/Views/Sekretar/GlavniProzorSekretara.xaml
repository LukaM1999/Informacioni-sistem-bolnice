<Window x:Class="InformacioniSistemBolnice.GlavniProzorSekretara"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
        Title="GlavniProzorSekretara" Height="350" Width="600">
    <Grid>
        <Button Content="Upravljanje nalozima pacijenata" HorizontalAlignment="Center" Margin="0,98,0,0" VerticalAlignment="Top" Width="300" Click="naloziPacijenata"/>
        <Button Content="Upravljanje alergenima" HorizontalAlignment="Center" VerticalAlignment="Top" Width="300" Click="alergeni_Click" Margin="0,145,0,0"/>
        <Button Content="Zakazivanje termina pacijentima" HorizontalAlignment="Center" Margin="0,194,0,0" VerticalAlignment="Top" Click="zakazivanjeTerminaPacijentima_Click" Width="300"/>
        <Button Content="Upravljanje vestima" HorizontalAlignment="Center" Margin="0,241,0,0" VerticalAlignment="Top" Click="vesti_Click" Width="300"/>
        <Button Content="Upravljanje urgentnim sistemom" HorizontalAlignment="Center" Margin="0,51,0,0" VerticalAlignment="Top" Width="300" Click="upravljanjeUrgentnimSistemom_Click"/>

    </Grid>
</Window>
