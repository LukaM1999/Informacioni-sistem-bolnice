<UserControl x:Class="InformacioniSistemBolnice.TerminiPacijentaProzorSekretara"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
       Height="480" Width="665" Background="LightGray" Margin="0,0,0,30">
    <Grid>
        <Canvas Height="600" Width="665">
            <DataGrid x:Name="listaZakazanihTermina" Height="400" Width="655" Canvas.Top="45" CanUserAddRows="False" Background="White" Margin="5,5,5,5"/>
            <Button x:Name="zakaziDugme" Content ="Zakazi" Canvas.Left="1" Canvas.Top="10" Width="60" Click="zakaziDugme_Click" Background="White" Margin="10,0,0,0"/>
            <Button Content="Pomeri" Canvas.Left="100" Canvas.Top="10" Width="60" Click="pomjeriTermin_Click" Background="White"/>
            <Button Content="Uvid" Canvas.Left="200" Canvas.Top="10" Width="60" Click="uvidUTerminPacijenta_Click" Background="White"/>
            <Button Content="Otkazi" HorizontalAlignment="Left" Canvas.Left="300" Canvas.Top="10" Width="60" Click="otkaziTermin_Click" Background="White"/>
        </Canvas>
       
    </Grid>
</UserControl>
