<UserControl x:Class="InformacioniSistemBolnice.TerminiPacijentaProzorSekretara"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
       Height="650" Width="1000" Background="LightGray" Margin="0,0,0,30">
    <Grid>
        <Canvas Height="650" Width="1000">
            <DataGrid x:Name="listaZakazanihTermina" Height="550" Width="900" Canvas.Top="57" CanUserAddRows="False" Background="White" Canvas.Left="48" CanUserResizeColumns="False"/>
            <Button x:Name="zakaziDugme" Content ="Zakazi" Canvas.Left="100" Canvas.Top="20" Width="80" Click="zakaziDugme_Click" Background="White" FontSize="14"/>
            <Button Content="Pomeri" Canvas.Left="200" Canvas.Top="20" Width="80" Click="pomjeriTermin_Click" Background="White" FontSize="14"/>
            <Button Content="Uvid" Canvas.Left="300" Canvas.Top="20" Width="80" Click="uvidUTerminPacijenta_Click" Background="White" FontSize="14"/>
            <Button Content="Otkazi" HorizontalAlignment="Left" Canvas.Left="400" Canvas.Top="20" Width="80" Click="otkaziTermin_Click" Background="White" FontSize="14"/>
        </Canvas>
       
    </Grid>
</UserControl>
