<UserControl x:Class="InformacioniSistemBolnice.UpravljanjeUrgentnimSistemomProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
        Height="475" Width="665" Background="LightGray" Margin="0,0,0,30">
    <Grid Height="475" Width="665">
        <StackPanel Margin="0,50,0,0">
            <DataGrid x:Name="ListaTermina" Height="430" Width="655" CanUserAddRows="False" Margin="5,5,5,5"/>
        </StackPanel>
        <Canvas x:Name="RukovanjePacijentima" />
        <Button Content="Zakazi hitan pregled" HorizontalAlignment="Left" VerticalAlignment="Top" Click="zakaziHitanTermin" Margin="10,10,0,0" Background="White"/>
        <Button Content="Kreiraj gostujuceg pacijenta" HorizontalAlignment="Left" Margin="150,10,0,0" VerticalAlignment="Top" Click="kreirajGostujucegPacijenta_click" Background="White"/>
    </Grid>
</UserControl>
